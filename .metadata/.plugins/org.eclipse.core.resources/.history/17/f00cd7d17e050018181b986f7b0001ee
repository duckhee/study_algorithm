#include <stdio.h>
#include <stdlib.h>

#define MAX       100

int Queue[MAX];
int Front, Rear;

void InitializeQueue(void);
void Put(int);
int Get(void);
void DisplayQueue(void);

void InitializeQueue(void)
{
	Front = Rear = 0;

}

void Put(int num)
{
	Queue[Rear++] = num;

	if(Rear>= MAX)
	{
		Rear = 0;
	}

}

int Get(void)
{
	int ret;
	ret = Queue[Front++];

	if(Front >= MAX)
	{
		Front = 0;
	}

	return ret;
}

void DisplayQueue(void)
{

}
